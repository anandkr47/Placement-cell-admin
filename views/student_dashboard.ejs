<style>
    /* Add your own custom CSS styles to enhance the appearance */

.container {
  padding: 20px;
}

.welcome-text {
  margin-bottom: 20px;
}

.table {
  margin-bottom: 20px;
}

.course-container {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}

.course {
  width: calc(33% - 20px);
  margin-bottom: 20px;
}

.course a {
  display: block;
  text-decoration: none;
  color: #333;
}

.course img {
  width: 100%;
  height: auto;
  margin-bottom: 10px;
}

.course h4 {
  font-size: 18px;
  margin-bottom: 5px;
}
.playlist-container {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}

.playlist {
  width: calc(33% - 20px);
  margin-bottom: 20px;
}

.playlist a {
  display: block;
  text-decoration: none;
  color: #333;
}

.playlist img {
  width: 100%;
  height: auto;
  margin-bottom: 10px;
}

.playlist h4 {
  font-size: 18px;
  margin-bottom: 5px;
}
h3 {
    color: white; /* Dark blue color */
  }
  h4{
    color:lightblue
  }
  td{
    color: black; /* Dark blue color */
  }
  .student-table {
    background-color: #f2f2f2;
  }

  
</style>

