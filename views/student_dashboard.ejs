<style>
    /* Add your own custom CSS styles to enhance the appearance */

.container {
  padding: 20px;
}

.welcome-text {
  margin-bottom: 20px;
}

.table {
  margin-bottom: 20px;
}

.course-container {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}

.course {
  width: calc(33% - 20px);
  margin-bottom: 20px;
}

.course a {
  display: block;
  text-decoration: none;
  color: #333;
}

.course img {
  width: 100%;
  height: auto;
  margin-bottom: 10px;
}

.course h4 {
  font-size: 18px;
  margin-bottom: 5px;
}

</style>

<header>
  <nav class="navbar navbar-expand-lg" style="background-color: wheat">
    <div class="container-fluid">
      <a class="navbar-brand h1" href="#">
        <i class="fas fa-laptop-code"></i> Placement Cell
      </a>
      <button
        class="navbar-toggler"
        type="button"
        data-bs-toggle="collapse"
        data-bs-target="#navbarSupportedContent"
        aria-controls="navbarSupportedContent"
        aria-expanded="false"
        aria-label="Toggle navigation"
      >
        <span class="navbar-toggler-icon"></span>
      </button>
      <div
        class="collapse navbar-collapse justify-content-around"
        id="navbarSupportedContent"
      >
          <ul class="nav text-purple">
            <li class="nav-item mx-2">
              <a class="nav-link text-purple" href="/student_dashboard/<%= student.email %>">Dashboard</a>
            </li>
            <li class="nav-item mx-2">
              <a class="nav-link" href="/student-profile">Profile</a>
            </li>
            <li class="nav-item mx-2">
              <a class="nav-link" href="/student-interview">My Interview</a>
            </li>
            <li class="nav-item mx-2">
              <a class="nav-link" href="/job-portal">Job Portal</a>
            </li>
          </ul>
        <h1> STUDENT DASHBOARD</h1>
          
        </div>
        
      </div>
    </nav>
  </header>
  <div class="container">
    <div class="row">
      <div class="col">
        <% if (student) { %>
        <div class="student-details">
          <h3 class="welcome-text">Welcome, <%= student.name %></h3>
          <table class="table student-table">
            <tbody>
              <tr>
                <td><strong>Name</strong></td>
                <td><%= student.name %></td>
              </tr>
              <tr>
                <td><strong>Email</strong></td>
                <td><%= student.email %></td>
              </tr>
              <tr>
                <td><strong>College</strong></td>
                <td><%= student.college %></td>
              </tr>
              <tr>
                <td><strong>Batch</strong></td>
                <td><%= student.batch %></td>
              </tr>
              <tr>
                <td><strong>Status</strong></td>
                <td><%= student.placement_status %></td>
              </tr>
              <tr>
                <td><strong>DSA score</strong></td>
                <td><%= student.dsa_score %></td>
                <td>
                  <a href="https://practice.geeksforgeeks.org/explore?page=1&sortBy=submissions&utm_source=geeksforgeeks&utm_medium=main_header&utm_campaign=practice_header" target="_blank" class="btn btn-primary">Practice DSA</a>
                </td>
              </tr>
              <tr>
                <td><strong>Web development score</strong></td>
                <td><%= student.webdev_score %></td>
                <td>
                  <a href="https://www.freecodecamp.org/news/tag/web-development/" target="_blank" class="btn btn-primary">Practice Web Development</a>
                </td>
              </tr>
              <tr>
                <td><strong>React score</strong></td>
                <td><%= student.react_score %></td>
                <td>
                  <a href="https://www.freecodecamp.org/learn/front-end-development-libraries/" target="_blank" class="btn btn-primary">Practice React</a>
                </td>
              </tr>
            </tbody>
          </table>
        </div>
        <% } %>
      </div>
    </div>
    <div class="row">
      <div class="col">
        <h3>Free Online Courses</h3>
        <div class="course-container">
          <div class="course">
            <a href="https://www.youtube.com/watch?v=8dWL3wF_OMw" target="_blank">
              <img src="https://img.youtube.com/vi/8dWL3wF_OMw/maxresdefault.jpg" alt="Course thumbnail">
              <h4>JavaScript Full Course for Beginners</h4>
            </a>
          </div>
          <div class="course">
            <a href="https://www.youtube.com/watch?v=RGOj5yH7evk" target="_blank">
              <img src="https://img.youtube.com/vi/RGOj5yH7evk/maxresdefault.jpg" alt="Course thumbnail">
              <h4>git and github - Crash Course</h4>
            </a>
          </div>
          <div class="course">
            <a href="https://www.youtube.com/watch?v=DLX62G4lc44" target="_blank">
              <img src="https://img.youtube.com/vi/DLX62G4lc44/maxresdefault.jpg" alt="Course thumbnail">
              <h4>React Tutorial for Beginners</h4>
          </a>
        </div>
        <!-- Add more courses here -->
      </div>
    </div>
  </div>
  </div>
  
  